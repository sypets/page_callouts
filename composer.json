{
	"name": "sypets/page-callouts",
	"type": "typo3-cms-extension",
	"description": "Add callouts to page module in TYPO3 backend",
	"license": ["GPL-2.0-or-later"],
	"keywords": [
		"TYPO3",
		"TYPO3 extension",
		"TYPO3 backend",
		"backend"
	],
	"authors": [
		{
			"name": "Sybille Peters",
			"email": "sypets@gmx.de",
			"role": "Developer"
		}
	],
	"config": {
		"sort-packages": true,
		"vendor-dir": ".Build/vendor",
		"bin-dir": ".Build/bin",
		"allow-plugins": {
			"typo3/class-alias-loader": true,
			"typo3/cms-composer-installers": true
		}
	},
	"require": {
		"php": "~7.4 || ~8.0 || ~8.1 || ~8.2",
		"typo3/cms-core": "^12.4",
		"typo3/cms-backend": "^12.4",
		"typo3/cms-fluid": "^12.4"
	},
	"require-dev": {
		"friendsofphp/php-cs-fixer": "@dev",
		"typo3/testing-framework": "@dev"
	},
	"conflict": {
		"qc/qc-be-pagelanguage": "<1.0.2"
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "page_callouts",
			"web-dir": ".Build/Web"
		}
	},
	"autoload": {
		"psr-4": {
			"Sypets\\PageCallouts\\": "Classes/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Sypets\\PageCallouts\\Tests\\": "Tests/"
		}
	},
	"scripts": {
		"post-autoload-dump": [
			"TYPO3\\TestingFramework\\Composer\\ExtensionTestEnvironment::prepare"
		]
	}
}
